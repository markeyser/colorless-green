{
    "servers": {
        "mermaid-validator": {
            "type": "stdio",
            "command": "/opt/homebrew/bin/npx",
            "args": [
                "-y",
                "@rtuin/mcp-mermaid-validator@latest"
            ]
        },
        "mermaid-doc-mcp-server": {
            "type": "stdio",
            "command": "/opt/homebrew/bin/uvx",
            "args": [
                "mermaid-doc-mcp-server@latest"
            ]
        },
        "filesystem": {
            "type": "stdio",
            "command": "/opt/homebrew/bin/npx",
            "args": [
                "-y",
                "--mount",
                "@modelcontextprotocol/server-filesystem",
                "${workspaceFolder}"
            ]
        },
        "git": {
            "type": "stdio",
            "command": "/opt/homebrew/bin/uvx",
            "args": [
                "mcp-server-git",
                "--repository",
                "${workspaceFolder}"
            ]
        }
    },
    "inputs": []
}
